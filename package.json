    "eslint": "^8.37.0",
    "eslint-plugin-vue": "^9.3.0",
    "gh-pages": "^6.1.0",
    "sass": "^1.60.0",
    "typescript": "^5.0.0",
    "unplugin-fonts": "^1.0.3",
